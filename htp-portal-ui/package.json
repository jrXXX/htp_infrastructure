{
  "name": "htp-portal-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "generate-api-client": "docker-compose up --build openapi_generator",
    "generate-holiday-api": "openapi-generator-cli generate -g typescript-angular -i http://localhost:8080/v3/api-docs -o src/app/api --type-mappings=date=Date",
    "generate-holiday-api-prod": "openapi-generator-cli generate -g typescript-angular -i https://htp-portal-backend.azurewebsites.net/v3/api-docs -o src/app/api --type-mappings=date=Date",
    "start-mock-backend": "json-server --watch src/assets/mock-data/db.json --port 8000",
    "start": "npm run generate-holiday-api-prod && ng serve",
    "build": "npm run generate-holiday-api && ng build",
    "build-docker": "npm run generate-api-client && ng build",
    "build-prod": "npm run generate-holiday-api-prod && ng build --configuration production",
    "build-prod-docker": "ng build --configuration production",
    "build-local-docker": "npm run generate-holiday-api-prod && ng build --configuration docker",
    "build-local": "npm run generate-holiday-api && ng build --configuration=local",
    "build-mock": "npm run generate-holiday-api && ng build --configuration=mock",
    "start-mock": "ng serve --configuration=mock",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cypress:open": "cypress open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@azure/msal-angular": "^2.0.0-beta",
    "@azure/msal-browser": "^2.14.1",
    "@fortawesome/angular-fontawesome": "^0.9.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@ngrx/effects": "^12.4.0",
    "@ngrx/router-store": "^12.4.0",
    "@ngrx/store": "^12.4.0",
    "@ngrx/store-devtools": "^12.4.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@openapitools/openapi-generator-cli": "~2.1.12",
    "@types/luxon": "^1.27.0",
    "dotenv": "^10.0.0",
    "luxon": "^1.27.0",
    "ng-image-slider": "^2.8.0",
    "ngx-sharebuttons": "^9.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.0",
    "@angular/cli": "~12.2.0",
    "@angular/compiler-cli": "~12.2.0",
    "@ngrx/schematics": "^12.4.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "cypress": "^8.4.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "json-server": "^0.16.3",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "^8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
